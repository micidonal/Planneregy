{
  "expo": {
    "name": "Planneregy",
    "slug": "Planneregy",
    "owner": "kmcdonald",
    "scheme": "planneregy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/svg/logo_Planneregy.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/fonts/**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.muc.planneregy",
      "buildNumber": "1.0.0",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
